{
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "name" : "Copy Paste Position Size",
  "identifier" : "com.sketchapp.copypastepositionsize",
  "version" : "1.4.0",
  "description" : "Easily copy and paste width, height, x & y values from and to objects",
  "authorEmail" : "mail@andrejilderda.nl",
  "author" : "Andre Jilderda",
  "commands": [
    {
      "name" : "Copy",
      "script" : "copy-paste-position-size.js",
      "handler" : "onCopy",
      "shortcut" : "ctrl shift c",
      "identifier" : "onCopy"
    },
    {
      "name" : "Paste Size",
      "script" : "copy-paste-position-size.js",
      "handler" : "onPasteSize",
      "shortcut" : "ctrl shift v",
      "identifier" : "onPasteSize"
    },
    {
      "name" : "Paste Width",
      "script" : "copy-paste-position-size.js",
      "handler" : "onPasteWidth",
      "shortcut" : "ctrl shift w",
      "identifier" : "onPasteWidth"
    },
    {
      "name" : "Paste Height",
      "script" : "copy-paste-position-size.js",
      "handler" : "onPasteHeight",
      "shortcut" : "ctrl shift h",
      "identifier" : "onPasteHeight"
    },
    {
      "name" : "Paste Width Porportionally",
      "script" : "copy-paste-position-size.js",
      "handler" : "onPasteWidthPorportionally",
      "shortcut" : "ctrl shift alt w",
      "identifier" : "onPasteWidthPorportionally"
    },
    {
      "name" : "Paste Height Porportionally",
      "script" : "copy-paste-position-size.js",
      "handler" : "onPasteHeightPorportionally",
      "shortcut" : "ctrl shift alt h",
      "identifier" : "onPasteHeightPorportionally"
    },
    {
      "name" : "Paste Position",
      "script" : "copy-paste-position-size.js",
      "handler" : "onPastePosition",
      "shortcut" : "ctrl shift p",
      "identifier" : "onPastePosition"
    },
    {
      "name" : "Paste X",
      "script" : "copy-paste-position-size.js",
      "handler" : "onPasteX",
      "shortcut" : "ctrl shift x",
      "identifier" : "onPasteX"
    },
    {
      "name" : "Paste Y",
      "script" : "copy-paste-position-size.js",
      "handler" : "onPasteY",
      "shortcut" : "ctrl shift y",
      "identifier" : "onPasteY"
    }
  ],
  "menu": {
    "title": "Copy Paste Position Size",
    "items": [
      "onCopy",
      "onPasteSize",
      "onPasteWidth",
      "onPasteHeight",
      "onPasteSizePorportionally",
      "onPasteWidthPorportionally",
      "onPasteHeightPorportionally",
      "onPastePosition",
      "onPasteX",
      "onPasteY"
    ]
  }
}