{
  "name": "sketch-copy-paste-position-size",
  "description": "Easily copy and paste width, height, x & y values from and to objects",
  "version": "1.5.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Sketch-Copy-Paste-Position-Size",
    "manifest": "src/manifest.json",
    "main": "sketch-copy-paste-position-size.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Andre Jilderda <mail@andrejilderda.nl>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajilderda/Sketch-Copy-Paste-Position-Size.git"
  }
}
